<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cs213.abhi.chess">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Game"></activity>
        <activity android:name=".Playback"
            android:parentActivityName=".Home"></activity>
        <activity android:name=".Save"></activity>
        <activity android:name=".ReplayGame"
            android:parentActivityName=".Playback"></activity>
    </application>

</manifest>